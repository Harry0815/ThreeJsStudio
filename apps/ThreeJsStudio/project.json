{
  "name": "ThreeJsStudio",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/ThreeJsStudio/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/ThreeJsStudio",
        "main": "apps/ThreeJsStudio/src/main.ts",
        "tsConfig": "apps/ThreeJsStudio/tsconfig.app.json",
        "assets": ["apps/ThreeJsStudio/src/assets"],
        "webpackConfig": "apps/ThreeJsStudio/webpack.config.js"
      },
      "configurations": {
        "development": {
          "args": ["node-env=development"]
        },
        "production": {
          "args": ["node-env=production"]
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "ThreeJsStudio:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "ThreeJsStudio:build:development"
        },
        "production": {
          "buildTarget": "ThreeJsStudio:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "options": {
        "passWithNoTests": true
      }
    }
  }
}
